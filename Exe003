program valorquadrado;

var

	valor, valor2, cont: longint;
	
begin

	read(valor);
	cont:= 0;
	
			while (valor <> 0) do
			begin
			
					read(valor2);
					
					if (valor2 <> valor * valor) then
						cont:= cont + 1;
					
					read(valor);
						
			end;
			
			if (cont > 0) then
				writeln('0')
			else 
				writeln('1');
		
end.
				
